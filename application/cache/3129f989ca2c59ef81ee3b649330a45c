    			setInterval(session_check, 6000);
		function session_check(){
			$.ajax({
				url: 'http://argshop.net/awfarshop/awfar/index.php/home/is_logged/',
				success: function(data) {
					if(data == 'yah!good'){}
					else if (data == 'nope!bad') {
						location.replace('http://argshop.net/awfarshop/awfar/');
					}
				},
				error: function(e) {
					console.log(e)
				}
			});
		}
				// Hide it after 3 seconds
        setTimeout(function(){
            $('html, body').css({
                'overflow': 'auto',
                'height': 'auto'
            });
            $('#preloader').fadeOut("slow");
        }, 1200);
        setTimeout(function(){
            if($('#layerslider').length){
                start_slider();
            }
            //$('#chatContainer').show('slow');
        }, 800);
    });

            function sound(type){
            document.getElementById(type).volume = 1;
            document.getElementById(type).play();
        }
