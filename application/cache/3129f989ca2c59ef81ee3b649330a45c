                                $('.srt').click(function(){
                                    var ty = $(this).data('val');
                                    var ny = $(this).html();
                                    $('#tryp').val(ty);
                                    $('.ppy').html(ny+' <span class="caret"></span>');
                                    if(ty == 'vendor'){
                                        $('#search_field').attr('placeholder','البحث البائعين حسب العنوان، الموقع، عنوان إلخ');
                                    } else if(ty == 'product'){
                                        $('#search_field').attr('placeholder','البحث عن منتج حسب العنوان، الوصف إلخ');
                                    } 
                                });
                            </script>
